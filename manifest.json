{
    "name": "Hello World Extension",
    "version": "1.0.0",
    "description": "A cool extension for Hello World printing",
    "permissions": ["storage", "tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "manifest_version": 3,
    "author": "Md. Sakir Ahmed",
    "action": {
        "default_popup": "index.html",
        "default_title": "Coolest extension",
        "default_icon": {
          "16": "assets/images/icons/16.png",
          "24": "assets/images/icons/24.png",
          "32": "assets/images/icons/32.png",
          "48": "assets/images/icons/48.png",
          "128": "assets/images/icons/128.png"
        }
    }
}